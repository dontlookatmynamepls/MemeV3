-- Đo FPS và tạo UI Frame
local player = game.Players.LocalPlayer

-- Tạo ScreenGui
local gui = Instance.new("ScreenGui")
gui.Name = "FPSGui"
gui.ResetOnSpawn = false
gui.Parent = player:WaitForChild("PlayerGui")

-- Tạo Frame
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 150, 0, 60)
frame.Position = UDim2.new(1, -10, 0, -20)
frame.AnchorPoint = Vector2.new(1, 0)
frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
frame.BackgroundTransparency = 0.3
frame.Active = true -- Cho phép nhận input
frame.Draggable = true -- Cho phép kéo
frame.Parent = gui

-- Bo góc
local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0.5, 0)
corner.Parent = frame

-- Ảnh icon
local imageLabel = Instance.new("ImageLabel")
imageLabel.Size = UDim2.new(0, 40, 0, 40)
imageLabel.Position = UDim2.new(0, 10, 0, 10)
imageLabel.Image = "rbxassetid://116721573962136"
imageLabel.BackgroundTransparency = 1
imageLabel.Parent = frame

-- Nhãn hiển thị FPS
local fpsLabel = Instance.new("TextLabel")
fpsLabel.Size = UDim2.new(0, 80, 0, 40)
fpsLabel.Position = UDim2.new(0, 55, 0, 10)
fpsLabel.BackgroundTransparency = 1
fpsLabel.TextScaled = true
fpsLabel.Font = Enum.Font.SourceSansBold
fpsLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
fpsLabel.Text = "FPS: 0"
fpsLabel.Parent = frame

-- Tạo nút click trùng với ảnh
local isWindowOpen = false
local clickArea = Instance.new("TextButton")
clickArea.Size = UDim2.new(0, 40, 0, 40) -- Trùng kích thước ảnh
clickArea.Position = UDim2.new(0, 10, 0, 10) -- Trùng vị trí ảnh
clickArea.BackgroundTransparency = 1
clickArea.Text = ""
clickArea.Parent = frame

clickArea.MouseButton1Click:Connect(function()
    isWindowOpen = not isWindowOpen
    if isWindowOpen then
        if Window and Window.Open then
            Window:Open()  -- Mở WindUI
        end
    else
        if Window and Window.Close then
            Window:Close()  -- Đóng WindUI
        end
    end
end)

-- Đo FPS
local RunService = game:GetService("RunService")
local fps = 60
local lastTime = tick()
local counter = 0

RunService.RenderStepped:Connect(function()
    counter += 1
    local now = tick()
    if now - lastTime >= 1 then
        fps = counter
        counter = 0
        lastTime = now

        fpsLabel.Text = "FPS: " .. tostring(fps)

        if fps >= 40 then
            fpsLabel.TextColor3 = Color3.fromRGB(0, 255, 0) -- xanh lá
        elseif fps >= 20 then
            fpsLabel.TextColor3 = Color3.fromRGB(255, 255, 0) -- vàng
        else
            fpsLabel.TextColor3 = Color3.fromRGB(255, 0, 0) -- đỏ
        end
    end
end)
